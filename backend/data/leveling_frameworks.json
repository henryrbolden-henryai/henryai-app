  "languagePatterns": {
    "entry_level": {
      "verbs": ["assisted", "supported", "helped", "contributed", "participated", "learned", "coordinated"],
      "descriptors": ["under guidance", "with supervision", "as part of team", "assigned to"]
    },
    "mid_level": {
      "verbs": ["managed", "led", "built", "developed", "implemented", "owned", "delivered", "designed"],
      "descriptors": ["independently", "end-to-end", "cross-functional", "on time"]
    },
    "senior_level": {
      "verbs": ["drove", "established", "architected", "mentored", "influenced", "defined", "scaled"],
      "descriptors": ["cross-team", "org-wide", "critical", "strategic", "complex"]
    },
    "principal_level": {
      "verbs": ["transformed", "pioneered", "shaped", "evangelized", "defined strategy"],
      "descriptors": ["company-wide", "industry-leading", "transformational", "board-level"]
    }
  },
